{
  "name": "zhisuan",
  "version": "1.0.0",
  "description": "一个智能计算器与记事本结合的工具，专为中国用户设计",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "stock": "ts-node src/stock/example.ts",
    "stock:open": "ts-node src/stock/open_data_example.ts",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "keywords": [
    "calculator",
    "notepad",
    "chinese",
    "smart-notes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/big.js": "^6.1.2",
    "@types/node": "^20.11.0",
    "@types/node-schedule": "^2.1.7",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.1",
    "big.js": "^6.1.1",
    "node-schedule": "^2.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "ava": "^4.0.1",
    "eslint": "^9.21.0",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0"
  },
  "ava": {
    "files": [
      "**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
